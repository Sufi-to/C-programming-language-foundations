#include<stdio.h>

int main(void){
    
    int expenses= 0;
    int total = 0;
    int num = -1;
    
    while (expenses!=num){
        scanf("%d", &expenses);
        if (expenses != -1){
           
           total = total + expenses; 
        }
        
        
    }
    printf("%d", total);
}
